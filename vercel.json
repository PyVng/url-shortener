{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "functions": {
    "server.js": {
      "maxDuration": 15
    }
  },
  "env": {
    "NODE_ENV": "production",
    "ACTIVE_DATABASE": "supabase_rest",
    "PGSSLMODE": "require"
  }
}
